{
  "name": "@jsse/pbfont",
  "type": "module",
  "version": "0.1.0",
  "author": "jessekrubin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jessekrubin/pbfont.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.18"
  },
  "scripts": {
    "gen": "npx buf generate proto && pnpm run fmt",
    "gen:clean": "npx buf generate proto --clean",
    "build-fast": "tsup",
    "build": "pnpm gen && tsup --dts && tsc",
    "build:tsc": "tsc",
    "fmt": "prettier --write src/**/*.{js,jsx,ts,tsx} package.json",
    "lint": "eslint .",
    "test": "vitest run",
    "cov": "vitest run --coverage",
    "tsup": "tsup",
    "vitest": "vitest --config vitest.config.ts --run",
    "vitest:watch": "vitest --config vitest.config.ts"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.32.2",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@jsse/eslint-config": "^0.1.15",
    "@jsse/tsconfig": "^0.0.3",
    "@types/node": "^20.14.2",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.56.0",
    "prettier": "~3.3.2",
    "tsup": "~8.1.0",
    "tsx": "~4.15.4",
    "typescript": "~5.4.5",
    "vitest": "~1.6.0"
  }
}
